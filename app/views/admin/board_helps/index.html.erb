
<table class="table table-hover">
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th></th>
    <th></th>
  </tr>
  <% for help in @helps %>
    <tr>
      <td><%= help.title %></td>
      <td><%= help.content %></td>
      <td><%= link_to "Edit", edit_admin_board_help_path(help), :class => 'btn btn-warning'%></td>
      <td><%= link_to "Destroy", [:admin, help], :confirm => 'Are you sure?', :method => :delete, :class => 'btn btn-danger' %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Admin/Help", new_admin_board_help_path %></p>
